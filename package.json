    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.6",
    "@types/multer": "^1.4.3",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^13.13.12",
    "@types/uuid": "^3.4.9",
    "autoprefixer": "^9.8.4",
    "concurrently": "^5.2.0",
    "eslint": "^7.4.0",
    "parcel-bundler": "^1.12.4",
    "postcss-import": "^12.0.1",
    "sass": "^1.26.9",
    "tailwindcss": "^1.4.6",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.50",
    "tslint": "^5.20.1",
    "typescript": "^3.9.6"
  },
  "dependencies": {
