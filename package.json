{
  "name": "lru-file-server",
  "version": "0.2.3-1",
  "description": "",
  "main": "./src/index.js",
  "scripts": {
    "dev": "concurrently \"parcel watch web/index.html\" \"ts-node-dev --project src/tsconfig.json --respawn src/index.ts\"",
    "build": "parcel build web/index.html && tsc --build src/tsconfig.json",
    "check": "tsc --project web/tsconfig.json --noEmit && tsc --project src/tsconfig.json --noEmit",
    "start": "node bin/index.js"
  },
  "bin": {
    "lruserve": "./bin/index.js"
  },
  "keywords": [],
