{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "curly": false,
        "arrow-parens": [
            false
        ],
        "max-classes-per-file": [
            true,
            999
        ],
        "no-console": [
            false
        ],
        "no-non-null-assertion": false,
        "object-literal-sort-keys": false,
        "quotemark": [
            true,
            "double"
        ],
        "trailing-comma": [
            true,
            {
                "multiline": "never",
                "singleline": "never"
            }
        ],
        "interface-name": false
    },
    "rulesDirectory": []
}
